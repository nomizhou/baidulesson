<!DOCTYPE html>
<html>
<head>
	<title></title>
	<meta charset="utf-8">
</head>
<body>
    <script type="text/javascript">
    	document.write("<table border='1px' cellpadding= '0' cellspacing='0'>");
    	for(var n=1; n<=9; n++){
    		document.write("<tr>");
    		for( var m=1 ;m<=n ; m++){
    			document.write("<td>");
    			document.write(n + "*" + m + "=" + n*m );
    			document.write("</td>");
    		};
    		document.write("</tr>");
    	};
    	document.write("</table>");
    </script>
</body>
</html>
